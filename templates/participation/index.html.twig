<!DOCTYPE html>

<title>Participation index</title>

{% block body %}
    <h1>Participation index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdPart</th>
                <th>DateParticipation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for participation in participations %}
            <tr>
                <td>{{ participation.idPart }}</td>
                <td>{{ participation.dateParticipation ? participation.dateParticipation|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_participation_show', {'idPart': participation.idPart}) }}">show</a>
                    <a href="{{ path('app_participation_edit', {'idPart': participation.idPart}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_participation_new') }}">Create new</a>
{% endblock %}
