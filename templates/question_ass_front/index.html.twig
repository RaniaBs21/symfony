{% extends 'basefront.html.twig' %}

{% block title %}QuestionAss index{% endblock %}

{% block body %}
    <h1>QuestionAss index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdQAss</th>
                <th>TypeQAss</th>
                <th>DescriptionQAss</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for question_ass in question_asses %}
            <tr>
                <td>{{ question_ass.idQAss }}</td>
                <td>{{ question_ass.typeQAss }}</td>
                <td>{{ question_ass.descriptionQAss }}</td>
                <td>
                    <a href="{{ path('app_question_ass_front_show', {'idQAss': question_ass.idQAss}) }}">show</a>
                    <a href="{{ path('app_question_ass_front_edit', {'idQAss': question_ass.idQAss}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_question_ass_front_new') }}">Create new</a>
{% endblock %}
